FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      zoneID;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{


    "(overset|overset_moving_object2)"
    {
        type            overset;
	value		$internalField;
    }

    "(maxX|minX|maxY|minY|maxZ|minZ|wall)"
    {
        type            zeroGradient;
    }

}

