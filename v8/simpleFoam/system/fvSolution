FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;
        tolerance       1e-06;
        relTol          0.01;
        preconditioner  DIC;
    }

    "(U|k|epsilon|omega|R|)"
    {
        solver          PBiCGStab;
        tolerance       1e-08;
        relTol          0.01;
        preconditioner  DILU;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 1;
    consistent      yes;

    residualControl
    {
        p               1e-15;
        U               1e-15;
    }
}


