FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|pcorr)"
    {
        solver          PCG;
        tolerance       1e-06;
        relTol          0.1;
        preconditioner  DIC;
    }
    
    "(p|pcorr)Final"
    {
        $p;
        tolerance       1e-06;
        relTol          0;
    }

    "(U|k|epsilon|omega|R|nuTilda)"
    {
        solver          PBiCGStab;
        tolerance       1e-05;
        relTol          0.1;
        preconditioner  DILU;
    }
   
    "(U|k|epsilon|omega|R|nuTilda)Final"
    {
        $U;
        tolerance       1e-06;
        relTol          0;
    }

}

PIMPLE
{
    nOuterCorrectors    2;
    nCorrectors         1;
    nNonOrthogonalCorrectors 1;
}


relaxationFactors
{
    fields
    {
        p               0.3;
    }
    equations
    {
        ".*"            0.7;
    }
}


